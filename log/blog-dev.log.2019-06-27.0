<script>
    //initial markdown editor
    var contentEditor;
    $(function() {
        contentEditor = editormd("md-content", {
            width   : "100%",
            height  : 640,
            syncScrolling : "single",
            path    : "/lib/editormd/lib/"
        });
    });
    $('.menu.toggle').click(function () {
        $('.m-item').toggleClass('m-mobile-hide');
    });

    $('.ui.dropdown').dropdown({
        on : 'hover'
    });
    $('#save-btn').click(function () {
        $('[name="published"]').val(false);
        $('#blog-form').submit();
    });
    $('#publish-btn').click(function () {
        $('[name="published"]').val(true);
        $('#blog-form').submit();
    });
    $('.ui.form').form({
        fields : {
            title : {
                identifier: 'title',
                rules:[{
                    type:'empty',
                    prompt:'Title: Please enter title of the blog'
                }]
            },
            content : {
                identifier: 'content',
                rules:[{
                    type:'empty',
                    prompt:'Title: Please enter content of the blog'
                }]
            },
            typeId : {
                identifier: 'type.id',
                rules:[{
                    type:'empty',
                    prompt:'Title: Please enter category of the blog'
                }]
            },
            indexPicture : {
                identifier: 'indexPicture',
                rules:[{
                    type:'empty',
                    prompt:'Title: Please enter indexPicture of the blog'
                }]
            }
        }
    });

</script>
