        $('[name="published"]').val(false);
        $('#blog-form').submit();
    });
    $('#publish-btn').click(function () {
        $('[name="published"]').val(true);
        $('#blog-form').submit();
    });
    $('.ui.form').form({
        fields : {
            title : {
                identifier: 'title',
                rules:[{
                    type:'empty',
                    prompt:'Title: Please enter title of the blog'
